  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'rust_ascii_game'",
      "cargo": {
        "args": ["build", "--bin=rust_ascii_game", "--package=rust_ascii_game"],
        "filter": {
          "name": "rust_ascii_game",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'rust_ascii_game'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=rust_ascii_game",
          "--package=rust_ascii_game"
        ],
        "filter": {
          "name": "rust_ascii_game",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
